#overtime-graphic
	svg
		border none
		box-shadow none
	.spacer
		height 300px
	.graphic-vis
		position absolute
		top 0
		margin-left 30rem
		transform translate3d(0, 0, 0)
		height 100vh
	.graphic-vis.is-fixed
		position fixed
	.graphic-vis.is-bottom
		top auto
		bottom 0
	.graphic
		width 100%
		position relative
